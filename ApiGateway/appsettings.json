{
  "ReverseProxy": {
    "Routes": {
      "sessions-route": {
        "ClusterId": "gamesession-cluster",
        "Match": {
          "Path": "/api/sessions/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "/sessions/{**catch-all}" }
        ]
      },
      "games-route": {
        "ClusterId": "gameengine-cluster",
        "Match": {
          "Path": "/api/games/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "/games/{**catch-all}" }
        ]
      }
    },
    "Clusters": {
      "gamesession-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://gamesession"
          }
        }
      },
      "gameengine-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://gameengine"
          }
        }
      }
    }
  }
}